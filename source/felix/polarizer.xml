double cta;
for(int i=0;i<n;i++){
 cta=c[i];
 x2[i]=x[i]+d*Math.cos(cta);
 y2[i]=y[i]+d*Math.sin(cta);
 x3[i]=x[i]+d2*Math.cos(cta);
 y3[i]=y[i]+d2*Math.sin(cta); 
 angle[i]=(int)(c[i]*180./Math.PI+0.5)+"";
}
if(show){
 I[1]=Math.cos(c[1])*Math.cos(c[1]);
 pass[1]=(int)(255*(1-I[1]));
 clr[1]=new java.awt.Color(color,color,color,pass[1]);
 I[2]=Math.cos(c[2]-c[1])*Math.cos(c[2]-c[1]);
 pass[2]=(int)(255*(1-I[2]));
 clr[2]=new java.awt.Color(color,color,color,pass[2]);
}else{
 clr[1]=new java.awt.Color(color,color,color,0);
 I[1]=1;
 I[2]=Math.cos(c[2])*Math.cos(c[2]);
 pass[2]=(int)(255*(1-I[2]));
 clr[2]=new java.awt.Color(color,color,color,pass[2]);
}
]]></Code>

