<Code><![CDATA[
//sin[1]=1.0;
double ddt=tmax/npt;
for(int i=0;i<npt;i++){
 wave[i][0]=i*ddt;
}
for(int i=0;i<n;i++){
 fid=(i/7);
 fjd=i%7;
 f[i]=fc[fid]*fn[fjd];
 f2[i]=fc[fid]*fn2[fjd];
  x[i]=xmin+(i+0.5)*w;
 x2[i]=x[i]+w/2;
 if(fn2[i%7]==0)show2[i]=false;
 else show2[i]=true;
}


]]></Code>
</Content>
