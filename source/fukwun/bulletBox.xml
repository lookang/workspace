 vy=L*omega*Math.sin(cta);
}
P2=(m+mb)*(L*L*omega*omega/2+g*L*(1-Math.cos(cta)));
km=0.5*m*(vx*vx+vy*vy);
kb=0.5*mb*(L*omega*L*omega);
U=(m+mb)*g*(y-yi);
