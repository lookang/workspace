<Name>init</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
if(_isPlaying())label="pause";
else label="play";

double c=Math.PI*2/n,ci;
int j;
for(int i=0;i<n;i++){
  j=i+1;
  cs[i]=-c*j;
  x[i]=Rs*Math.sin(c*j);
  y[i]=Rs*Math.cos(c*j);
  msg[i]=j+"";
}
c=pi2/n2;
for(int i=0;i<n2;i++){
 ci=c*i;
 xs[i]=D2*Math.sin(ci);
 ys[i]=D2*Math.cos(ci);
 dxs[i]=-ds*Math.sin(ci);
 dys[i]=-ds*Math.cos(ci);
 if(i%5==0){
  dxs[i]=2*dxs[i];
  dys[i]=2*dys[i];
 }
}
java.util.GregorianCalendar Calendar=new java.util.GregorianCalendar();
vs=Calendar.get(Calendar.SECOND);
vm=Calendar.get(Calendar.MINUTE);
vh=Calendar.get(Calendar.HOUR_OF_DAY);

set();


]]></Code>
