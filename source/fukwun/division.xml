<Code><![CDATA[
int vid;
public int getpower10(int a){
 return (int)(Math.log(a)/Math.log(10));// power of 10
}
public int getNumber (int a,int b) {
 id10=getpower10(a);
 vid=1;
 for(int i=0;i<id10;i++)vid*=10;
 for(shift=0;shift<id10;){
  if(a/vid>=b)return a/vid;
  vid/=10;
  shift++;
 }
 return a;
}
]]></Code>

