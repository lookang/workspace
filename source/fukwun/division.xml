public int setini (int ii,int[] d) {
int  id=getpower10(ii);
factor=1;
for(int i=0;i<id;i++)factor*=10;
cv3=ii;
for(int i=0;i<id+1;i++){
 d[i]=(int)(cv3/factor);
 cv3-=d[i]*factor;
 factor/=10;
}

return id;
}
]]></Code>

