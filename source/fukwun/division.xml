<Code><![CDATA[
int o;
int factor;
public int generateNumber (int n,int[] d) {
 d[0]=(int)(Math.random()*9)+1; 
 if(n>1)for(int i=1;i<n;i++){
   d[i]=(int)(Math.random()*10);
  }
  o=d[n-1];
  factor=10;
  if(n>1)for(int i=n-2;i>-1;i--){
    o+=factor*d[i];
    factor*=10;
  }
  return o;
}
public int getV10(int v,int id){
 if(id>0)for(int i=1;i<id;i++){
  v*=10;
 }
 return v;
}

]]></Code>

</Content>
