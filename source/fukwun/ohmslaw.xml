 }else if(mode==1){
   y[i]=y[i-1]-d;
   if(y[i]<-h2){
     x[i]=w2-(-h2-y[i]);
     y[i]=-h2;
     vx[i]=-1;
     vy[i]=0;
     mode=2;
   }else{
     vx[i]=0;
     vy[i]=-1;
     x[i]=w2;
   }
 }else if(mode==2){
