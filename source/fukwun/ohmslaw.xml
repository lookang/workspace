<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void checkxy (double xc,double x[],double y[],double vx[],double vy[]) {
 for(int i=0;i<np;i++){
  if(vx[i]>0){//mode 0
   if(x[i]>xc+w2){
     y[i]=h2-(x[i]-xc-w2);
     x[i]=xc+w2;
     vx[i]=0;
     vy[i]=-1;
   }else if(x[i]<xc-w2){
     y[i]=h2-(xc-w2-x[i]);
     x[i]=xc-w2;
     vx[i]=0;
     vy[i]=1;
   }
  }else if(vy[i]<0){// mode 1
    if(y[i]<-h2){
      x[i]=xc+w2-(-h2-y[i]);
      y[i]=-h2;
      vx[i]=-1;
      vy[i]=0;
    }else if(y[i]>h2){
      x[i]=xc+w2-(y[i]-h2);
      y[i]=h2;
      vx[i]=1;
      vy[i]=0;
    }
  }else if(vx[i]<0){//mode 2
    if(x[i]<xc-w2){
     y[i]=-h2+xc-w2-x[i];
     x[i]=xc-w2;
     vy[i]=1;
     vx[i]=0;
    }else if(x[i]>xc+w2){
     y[i]=-h2+x[i]-xc-w2;
     x[i]=xc+w2;
     vx[i]=0;
     vy[i]=-1;
    }
  }else if(vy[i]>0){//mode 3
   if(y[i]>h2){
    x[i]=xc-w2+y[i]-h2;
    y[i]=h2;
    vx[i]=1;
    vy[i]=0;
   }else if(y[i]<-h2){
    x[i]=xc-w2+-h2-y[i];
    y[i]=-h2;
    vx[i]=-1;
    vy[i]=0;
   }
  }
 }
}

]]></Code>

</Content>
</Osejs.Model.Library.Page>
