<Type>LIBRARY_EDITOR</Type>
<Name>drag</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void drag () {
dxp=2*x1*k;
dyp=2*y1*k;

for(int i=0;i<n;i++){
 for(int j=0;j<n;j++){
  r=Math.sqrt((i-n2)*(i-n2)+(j-n2)*(j-n2));
  data[i][j]=Math.cos(k*r+dxp-dyp)*Math.cos(k*r+dxp-dyp);
 }
}
}
]]></Code>

</Content>
