<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
double r2,r3,f;
// caluclate magnetic field at point xp,yp use Biot-Savart Law Fproportional to dlXr/r^3
public double calFx (double xp,double yp) {
 f=0;
 for(int i=0;i<np;i++){
  for(int j=0;j<nc;j++){
   r2=(xp-xc[j])*(xp-xc[j])+(yp-yc[i])*(yp-yc[i])+zc[i]*zc[i];
   r3=r2*Math.sqrt(r2);
   f-=(yc[i]*(yp-yc[i])-zc[i]*zc[i])/r3;
  }
 }
 return f;
}
public double calFy (double xp,double yp) {
 f=0;
 for(int i=0;i<np;i++){
  for(int j=0;j<nc;j++){
   r2=(xp-xc[j])*(xp-xc[j])+(yp-yc[i])*(yp-yc[i])+zc[i]*zc[i];
   r3=r2*Math.sqrt(r2);
   f+=yc[i]*(xp-xc[j])/r3;
  }
 }
 return f;
}

]]></Code>
</Content>
