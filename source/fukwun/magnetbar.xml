<Osejs.Model.Initialization.Page>
<Type>CODE_EDITOR</Type>
<Name>Init Page</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
// magnetic bar is simulated with current loops
for(int i=0;i<nc;i++)xc[i]=w-i*dx;  // assume there are nc current loops, xc[i] is the x-coordinate of the coil
for(int i=0;i<np;i++){// here calculate the (y,z) coordinate for each loop, the circle is cut into np segments 
 yc[i]=size2*Math.cos(i*dc);
 zc[i]=size2*Math.sin(i*dc);
}
drag();// calculate magnetic field at magnetic nieedle position
]]></Code>
</Content>
