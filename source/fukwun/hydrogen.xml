<Code><![CDATA[
public Object color (double l) {
 l=l*100;
 if(l<380)return new java.awt.Color(128,128,128);
 else if(l<450)return new java.awt.Color(255,0,255);
 else if(l<495)return new java.awt.Color(0,0,255);
 else if(l<570)return new java.awt.Color(0,255,0);
 else if(l<590)return new java.awt.Color(255,255,0);
 else if(l<620)return new java.awt.Color(255,128,0);
 else if(l<750)return new java.awt.Color(255,0,0);
 return new java.awt.Color(32,32,32);
}
]]></Code>
