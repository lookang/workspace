public void calc () {
color[ns-1]=black;
color[n-1]=green;
show[ns-1]=false;
show[n-1]=true;
ns=n;
x1=0.8*xmax*scale;
L=0.15*xmax*scale;

for(int i=0;i<nm;i++){
 id=i+1;
 id2=id*id;
 ey[i]=emin*scale/id2;
 r[i]=-emin*id2/(2*nm*nm);
 r2[i]=r[i]*2;
 ty[i]=yc-r[i];
 omega=Math.sqrt(r3w2/(r[n-1]*r[n-1]*r[n-1]));
}
l_E="E("+n+")=-13.6eV/"+(n)*(n)+"="+dvalue(-13.6/(n*n))+" eV";
l_R="R("+n+")=0.529A^{o} *"+n*n+"="+dvalue(0.529*n*n)+"A^{o}";
R=r[n-1];
D=R/8;
fratio=1.;
}
double dv=0.0;
public double dvalue(double v){
 if(v>0)dv=0.5;
 else if(v<0)dv=-0.5;
 else dv=0.;
 return (int)(v*1000+dv)/1000.;
}


]]></Code>

</Content>
