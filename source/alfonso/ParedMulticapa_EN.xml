<Code><![CDATA[
public void changeTint () {
 String s;
 int l;
 T1=T1*1.0000000000001;
 if (T1>5) T1=5;
 if (T1<-35) T1=-35;
 s= String.valueOf(T1);
 if (s.length()<4)
   l = s.length();
 else 
   l = 4;

 
 Tint = String.valueOf(T1).substring(0,l) + " ºC";
  
}

public void changeText () {
 String s;
 int l;
 T4=T4*1.0000000000001;
 if (T4>50) T4=50;
 if (T4<10) T4=10;
 s= String.valueOf(T4);
 if (s.length()<5)
   l = s.length();
 else 
   l = 5;

 Text =  s.substring(0,l) + " ºC";

}

public void changeLa(){
 if (La+Lb+Lc>Lmax) La=Lmax-Lb-Lc;
 if (La<0.005) La = 0.005;
 dibujaPared();
}


public void changeLb(){
 if (La+Lb+Lc>Lmax) Lb=Lmax-La-Lc;
 if (Lb<0.005) Lb = 0.005;
 dibujaPared();
}


public void changeLc(){
 if (La+Lb+Lc>Lmax) Lc=Lmax-Lb-La;
 if (Lc<0.005) Lc = 0.005;
 dibujaPared();
}
]]></Code>

</Content>
