<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void ChangeTheta () {
theta = thetaInicial*Math.PI/180; omega = 0;
}

public void ChangeL () {
K  = Math.sqrt(g/L);
theta = thetaInicial*Math.PI/180; omega = 0;
}

]]></Code>

