<Type>CODE_EDITOR</Type>
<Name>vista amortiguador</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
x_amortiguadorFijo[0] = LB;
x_amortiguadorFijo[1] = LB;
x_amortiguadorFijo[2] = LA;
x_amortiguadorFijo[3] = LA;
x_amortiguadorFijo[4] = 0;
x_amortiguadorFijo[5] = LA;
x_amortiguadorFijo[6] = LA; 
x_amortiguadorFijo[7] = LB; 
x_amortiguadorFijo[8] = LB;

y_amortiguadorFijo[0] = yAmortiguador + LD/2;
y_amortiguadorFijo[1] = yAmortiguador + LC/2;
y_amortiguadorFijo[2] = yAmortiguador + LC/2;
y_amortiguadorFijo[3] = yAmortiguador;
y_amortiguadorFijo[4] = yAmortiguador;
y_amortiguadorFijo[5] = yAmortiguador;
y_amortiguadorFijo[6] = yAmortiguador - LC/2;
y_amortiguadorFijo[7] = yAmortiguador - LC/2;
y_amortiguadorFijo[8] = yAmortiguador - LD/2 ;


]]></Code>

</Content>
</Osejs.Model.Initialization.Page>
<Osejs.Model.Initialization.Page>
<Type>CODE_EDITOR</Type>
<Name>estado automata</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
 if ( v1 > v2 ) {
   Detras = 1;
 } else if ( v1 < v2 ) {
   Adelante = 1;
 } else {
   Acoplo = 1;
 }
]]></Code>

</Content>
</Osejs.Model.Initialization.Page>
</Osejs.Model.Initialization>
<Osejs.Model.Evolution>
<Osejs.Model.Evolution.Page>
<Type>ODE_EDITOR</Type>
<Name>cálculo estados</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<IndependentVariable>t</IndependentVariable>
<Increment>incremento_t</Increment>
<Rate state="x1">v1;</Rate>
<Rate state="v1">(Detras+Adelante)*(-k1*(x1-x0_muelle1)-b*v1-(Rv*(v1-v2)+(Detras-Adelante)*Rm))/m1+Acoplo/(m1+m2)*(-k1*(x1-x0_muelle1)-b*v1);</Rate>
<Rate state="x2">v2;</Rate>
<Rate state="v2">(Detras+Adelante)*(Rv*(v1-v2)+(Detras-Adelante)*Rm)/m2+Acoplo/(m1+m2)*(-k1*(x1-x0_muelle1)-b*v1);</Rate>
<Rate state=""></Rate>
<Method>MidPoint</Method>
<Tolerance></Tolerance>
<Comment><![CDATA[]]></Comment>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>CODE_EDITOR</Type>
<Name>choque pared</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
if ( x1<LA+LP ) {
   x1 = LA + LP;
   v1 = 0;
   }

if ( x1> LB+LP ) {
   x1 = LB + LP;
   v1 = 0;
   }

if (x1 < LB ) {
      x1 = LB;
      v1 = 0;
   }
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>CODE_EDITOR</Type>
<Name>modo</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
if ( Detras == 1 && v1 <= v2 ) {
   Detras = 0;
   Acoplo = 1;
   v2 = v1;
   }

if ( Adelante == 1 && v1 >= v2 ) {
   Adelante = 0;
   Acoplo = 1;
   v2 = v1;
   }

]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
<Osejs.Model.Evolution.Page>
<Type>CODE_EDITOR</Type>
<Name>masa2 sobre masa1</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
 if ( x2 < x1 )                 {   x2 = x1;   }
 if ( x2 > x1 + Lmasa1 ) {   x2 = x1 + Lmasa1; }
]]></Code>

</Content>
</Osejs.Model.Evolution.Page>
</Osejs.Model.Evolution>
<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>modelo matematico</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
Fmuelle1  = -k1 * ( x1 - x0_muelle1 );
Famortiguador   = -b * v1;
v = v1 - v2;

if (Adelante == 1)  {   Ff = Rv * v - Rm;    }
if (Detras == 1)     {   Ff = Rv * v + Rm;   }
if (Acoplo == 1)     {   Ff = m2 / (m1+m2)*(Fmuelle1 + Famortiguador);   }

if ( Acoplo == 1 && Ff > R0  ) { Acoplo = 0;  Detras    = 1;  Ff = Rv * v + Rm; }
if ( Acoplo == 1 && Ff < -R0 ) { Acoplo = 0;  Adelante = 1;  Ff = Rv * v - Rm; }

Fmasa1  = Fmuelle1 + Famortiguador-Ff;
Fmasa2  = Ff;
a1   = Fmasa1 / m1;
a2   = Fmasa2 / m2;

]]></Code>

</Content>
</Osejs.Model.Constraints.Page>
</Osejs.Model.Constraints>
<Osejs.Model.Library>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>mover masa1</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void mover_masa1 () {
   yMasa1 = 0;
   if ( x1 > LB + LP ) {
      x1 = LB + LP;
      }
   if (x1 < LA + LP ) {
      x1 = LA + LP;
      }
   if (x1 < LB) {
      x1 = LB;
      }
   if ( x2 < x1 ) {
      x2 = x1;
      }
   if ( x2 > x1 + Lmasa1 ) {
      x2 = x1 + Lmasa1;
      }
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>mover masa2</Name>
<Active>true</Active>
<Visible>true</Visible>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void mover_masa2 () {
   yMasa2 = 0.85;
   if ( x2 < x1 ) {
      x2 = x1;
      }
   if ( x2 > x1 + Lmasa1 ) {
      x2 = x1 + Lmasa1;
      }
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
</Osejs.Model.Library>
