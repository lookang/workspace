public void changeFunction() {
	// with change of function type is fetched
	if(functionList != null) {
		// a^Where is the selectoed function in the function list ?
		int pos = functionList.indexOf(Fx.trim());

		if(pos >= 0) {
			// fetch description
			description = descriptionList.get(pos);
			// set start value
			Double mi = minList.get(pos);
      	                if(mi != null) {
      	                   min = mi.doubleValue(); 
			}
                        // set end value
                        Double ma = maxList.get(pos);
      	                if(ma != null) {
      	                   max = ma.doubleValue(); 
			}
                        // set initial value
			Double sw = startwertList.get(pos);
      	                if(sw != null) {
      	                    ya0 = sw.doubleValue(); 
			}
		}
	}
	else {
		System.out.println("leere functionList");
	}
        /* die Funkion fetch derivative */
        f = _view.getFunction("FieldFx2","function");
	double h = (Math.abs(min) + Math.abs(max)) / (q*5); // step for approximation
	 // calculate values
        calculate();
}
]]></Code>
