for (int i=positives; i<n; i++) {
  connect[i] = true;
  if (x[i] > xmax+diameter[i]) {
    x[i] = xmin-diameter[i];
    if (force>0) vx[i] = 0;
    connect[i] = false;
    if (i==positives) _resetView();
  }
  if (x[i] < xmin-diameter[i]) { x[i] = xmax+diameter[i]; connect[i] = false; }
  if (y[i] > ymax+diameter[i]) { y[i] = ymin-diameter[i]; connect[i] = false; }
  if (y[i] < ymin-diameter[i]) { y[i] = ymax+diameter[i]; connect[i] = false; }
};

