<Content>
<PreliminaryCode>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
System.arraycopy(zeros,0,forceX,0,forceX.length);
System.arraycopy(zeros,0,forceY,0,forceY.length);

//fuerzas de muelle y de campo respectivamente
for(int i=0;i<n;i++){
  forceX[i] = xforce(x,y,i);
  forceY[i] = yforce(x,y,i);
}
//fuerzas de Lennard-Jones
for(int i=0; i<n; i++) {
  for(int j=i+1; j<n; j++) {
    double dx = x[i] - x[j];
    double dy = y[i] - y[j];
    double r1 = dx*dx + dy*dy;
    double r = Math.sqrt(r1);

    double aux = Math.pow(r,13);
    double aux2 = Math.pow(r,7);
    
//   if(aux<1.0e-6)System.out.println("UPSSS "+ i +" , " + j);
    double fx = 4*epsilon*(12*sigma12/aux - 6*sigma6/aux2)*dx/r;

    double fy = 4*epsilon*(12*sigma12/aux - 6*sigma6/aux2)*dy/r;

//limitador de aceleración
//    if(fx>1.0e6)fx=1.0e3;
//   if(fy>1.0e6)fy=1.0e3;
    
    forceX[i] -= fx;
    forceY[i] -= fy;
    forceX[j] += fx;
    forceY[j] += fy;
 
  }


}


]]></Code>
