<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Cons Page</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
q[0] = q0;
q[1] = q1;
q[2] = q2;
q[3] = q3;
q[4] = q4;

// Field and potential at the test charge position

Enetx = 0.0;
Enety = 0.0;
Vnet = 0.0;

for (i = 0; i < n; i++)
{
rSquared=(x[i]-xtest)*(x[i]-xtest)+(y[i]-ytest)*(y[i]-ytest);
Ex[i]=k*q[i]*(xtest-x[i])/(rSquared*Math.sqrt(rSquared));
Ey[i]=k*q[i]*(ytest-y[i])/(rSquared*Math.sqrt(rSquared));
Enetx = Enetx + Ex[i];
Enety = Enety + Ey[i];
Vnet = Vnet + k*q[i]/(Math.sqrt(rSquared));
}

Enet = Math.sqrt(Enetx*Enetx+Enety*Enety);

// Field for the vectors
for (i = 0; i < nVectors; i++)
{
  for (j = 0; j < nVectors; j++)
  {
    vectorIndex = i+nVectors*j;
    Fieldx[vectorIndex] = 0.0;
    Fieldy[vectorIndex] = 0.0;
  }
}


for (i2 = 0; i2 < n; i2++)
{
  for (i = 0; i < nVectors; i++)
  {
    for (j = 0; j < nVectors; j++)
    {
      vectorIndex = i+nVectors*j;
      rSquared=(x[i2]-Vectorx[vectorIndex])*(x[i2]-Vectorx[vectorIndex])+(y[i2]-Vectory[vectorIndex])*(y[i2]-Vectory[vectorIndex]);
      Fieldx[vectorIndex]=Fieldx[vectorIndex]+k*q[i2]*(Vectorx[vectorIndex]-x[i2])/(rSquared*Math.sqrt(rSquared));
      Fieldy[vectorIndex]=Fieldy[vectorIndex]+k*q[i2]*(Vectory[vectorIndex]-y[i2])/(rSquared*Math.sqrt(rSquared));
    }
  }
}

for (i = 0; i < nVectors; i++)
{
  for (j = 0; j < nVectors; j++)
  {
    vectorIndex = i+nVectors*j;
    Fieldmag=Math.sqrt(Fieldx[vectorIndex]*Fieldx[vectorIndex]+Fieldy[vectorIndex]*Fieldy[vectorIndex]);
    if (Fieldmag > 0.0)
     {
       Fieldx[vectorIndex]=0.8*Fieldx[vectorIndex]/Fieldmag;
       Fieldy[vectorIndex]=0.8*Fieldy[vectorIndex]/Fieldmag;
     }
    posx[vectorIndex]=Vectorx[vectorIndex]-0.5*Fieldx[vectorIndex];
    posy[vectorIndex]=Vectory[vectorIndex]-0.5*Fieldy[vectorIndex];
    Fieldmag=Math.min(10000,Fieldmag);
    redness=255-(int)(255*Fieldmag/10000);
    colorVectors[vectorIndex]= new java.awt.Color(redness,redness,redness);
  }
}

// Setting the colors (red for +, blue for -)
for (i = 0; i < n; i++)
{
  if (q[i] > 0.0)
  {
  redness = 250;
  greenness = 0;
  blueness = 0;
  colorball[i] = new java.awt.Color(redness,greenness,blueness);
  }
  else
  {
    if (q[i] < 0.0)
    {
    redness = 100;
    greenness = 100;
    blueness = 250;
    colorball[i] = new java.awt.Color(redness,greenness,blueness);
    }
    else
    {
    redness = 200;
    greenness = 200;
    blueness = 200;
    colorball[i] = new java.awt.Color(redness,greenness,blueness);
    }  
  }
}

]]></Code>

