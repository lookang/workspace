<Code><![CDATA[
public void dragBlock () {
  if(Math.abs(x)<range/100.){
    cta=0;
  }else if(x>0){
     a=Math.atan2(y,w2-x);
     cta=cta0-a;
     if(cta>0){
      cta=0;
     }else xn=w2;
  }else if(x<0){
     a=Math.atan2(y,x+w2);
     cta=a-cta0;
     if(cta<0){
      cta=0;
    }else     xn=-w2;
  }
  omega=0;
}
]]></Code>
</Content>
