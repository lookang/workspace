omega2=omega*omega;
NY=m*h2*omega2;
if(Math.abs(Math.sin(cta))<1.e-4){
  cta=0;
  omega=0;
  _pause();
}else if(cta<0){
     xn=w2;
     a=cta0-cta;
     x=w2-r*Math.cos(a);
     y=r*Math.sin(a);
     vx=-r*omega*Math.sin(a);
     vy=-r*omega*Math.cos(a);
     NX=-m*w2*omega2;
     if(x>w2)NX=-NX;
}else if(cta>0){
     xn=-w2;
     a=cta0+cta;
     x=r*Math.cos(a)-w2;
     y=r*Math.sin(a);
     vx=-r*omega*Math.sin(a);
     vy=r*omega*Math.cos(a);
     NX=m*w2*omega2;
     if(x<-w2)NX=-NX;
}else {
   xn=x=0;
   y=h2;
}
if(y<w2&& _isPlaying()){
 if(cta<0)cta=-Math.PI/2;
 else cta=Math.PI/2;
 y=w2;
 playpause();
}


