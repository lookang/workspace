public void recordData () {
 collectData = false; // needed to prevent instantaneous data collect
//  collectData = true; // needed to prevent instantaneous data collect
magRow[0]=q;
magRow[1]=xDetector;
  magRow[2]=yDetector;
  double r=Math.sqrt(xDetector*xDetector+yDetector*yDetector);
  magRow[3]=r;
  magRow[4]= getE(xDetector,yDetector)[2];
  //adding points to a trail to be accessed by dataTool
//  _view.xytrail.addPoint(xDetector, r);
//  _view.Etrail.addPoint(r,getE(xDetector,yDetector)[2]);
  collectData = true; // needed to prevent instantaneous data collect
// collectData = false; // needed to prevent instantaneous data collect
}

]]></Code>

