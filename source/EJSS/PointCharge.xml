<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
//this section of the model is correct
//it gets the electric field components from the "Get E value"
//page to generate the picture of the field vectors
public void computeField () {
  for (int i=0; i<nx; i++) {
    double x=_view.vectorField2D.indexToX(i);
    for (int j=0; j<ny; j++) {
      double y=_view.vectorField2D.indexToY(j);
      double[] e=getE(x,y);//  Gets the components and magnitude
      xField[i][j] = e[0];
      yField[i][j] = e[1];
      elecField[i][j]=e[2];
    }
  }
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>Get E value</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
// computes electric field
public double[] getE(double x, double y) {
  double[] e=new double[4];
      double r2=x*x+y*y;
      double r=Math.sqrt(r2);
      double cos=x/r;
      double sin=y/r;
      double r3=r2*r;
      if(r2==0) {
        e[0]=0;
        e[1]=0;
      } else {
        e[0]=q*x/r3;  // x compondent
        e[1]=q*y/r3;  // y component
              }
    e[2]=Math.sqrt(e[0]*e[0]+e[1]*e[1]);  // magnitude
  return e;
}
]]></Code>

</Content>
</Osejs.Model.Library.Page>
