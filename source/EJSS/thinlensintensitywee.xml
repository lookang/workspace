<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Cons Page</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
double vx=0,vy=0,v,x2,y2,ddt;
int count;
count=0;
if(t>dt)for(int i=0;i<n;i++){
 if(block[i]==1){//blocked 
   count++;
 }else if(xp1[i]<x){
   vx=dxp1[i]; vy=dyp1[i];
 }else if(xp1[i]<xmax){
   if(dxp2[i]<0){
   vx=-dxp2[i]; vy=-dyp2[i];
   }else{
    vx=dxp2[i]; vy=dyp2[i];
   }
 }else{// pass xmax
   vx=dxp1[i]; vy=dyp1[i];
   count++;
 }

  if(block[i]<1){//not blocked
    v=Math.sqrt(vx*vx+vy*vy);
    vxp=vx*c/v;
    vyp=vy*c/v;
    x2=xp1[i]+vxp*dt;
    y2=yp1[i]+vyp*dt;
    if((bx-xp1[i])*(bx-x2) <=0 ){// may be passing block
      ddt=(bx-xp1[i])/vxp;
      if( (y2-bh)*(y2+bh)>=0) {// blocked
        block[i]=1;
        x2=bx;
        y2=yp1[i]+vyp*ddt;
         count++;
         bcount++;
         blocked=true;
         cvalue=(int)(255.*(n-bcount)*(n-bcount)/n/n);
         clr=new java.awt.Color(0,0,cvalue,cvalue);
      }
    }else if((x-xp1[i])*(x-x2) <0 ){//passing lens
      ddt=(x-xp1[i])/vxp;
      x2=x;
      y2=yp1[i]+vyp*ddt; 
    }
      xp1[i]=x2;
      yp1[i]=y2;
 }
}
Ivalue=(int)(cvalue*1000./255.)/10.;
Itext=I_intensity+"="+Ivalue+"%";
if(count==n){
 if(autoy){
  ly+=ldy;
  if(ly>h){
   ly=h;
  ldy=-Math.abs(ldy); 
  }else if(ly<0){
   ly=0;
   ldy=Math.abs(ldy);
  }
 }
 _initialize();
}
]]></Code>
