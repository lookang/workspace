public void updateaction () {
cvalue=255;
double dy=lh/n,lymin;
zero=0.;
x1=x+f;
x2=x-f;
if(f!=p)q=x+(x-p)*f/(x-p-f);
else q=1.e5;
h2=q/p*h;
lymin=-lh/2+dy/2-ly;
for(int i=0;i<n;i++){
  xp1[i]=p;
  yp1[i]=ly;
  dxp1[i]=x-p;
  dyp1[i]=lymin+i*dy;
  xp2[i]=q;
  yp2[i]=h2;
   dxp2[i]=x-q;
   dyp2[i]=dyp1[i]+ly-ly*h2/h;
  block[i]=-1;
  stage[i]=0;
}
bcount=0;
t=0;
clr=new java.awt.Color(0,0,255);
 _resetView();
}
