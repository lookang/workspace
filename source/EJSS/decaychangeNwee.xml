<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
n = ni; //added to initialise to be able to choose n
n2=n*n;
count=n2;
dx=(xmax-xmin)/n;
dy=(ymax-ymin)/n;
for(int i=0;i<n2;i++){
 px[i]=xmin+(i%n) *dx;
 iy=i/n;
 py[i]=ymin+ iy *dy;
 clr[i]=gray;
}
for(int i=0;i<n;i++){
 for(int j=0;j<n;j++) {
  data[i][j]=0;
 }
}
//count=n*n;
ratio=1.;
t=0;



