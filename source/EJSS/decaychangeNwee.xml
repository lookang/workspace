<Name>Cons Page</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
if(_isPlaying()){
for(int i=0;i<n;i++){
for(int j=0;j<n;j++){
  if(data[i][j]==0){
     if(Math.random()<p){
      data[i][j]=1;
      count=count-1;
      clr[i+j*n]=red;
     }
  }
}
}
if(count<1)_pause();
ratio=count*1.0/n2;
}

]]></Code>
