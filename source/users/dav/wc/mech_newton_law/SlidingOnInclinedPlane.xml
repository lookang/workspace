<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>Drag Ramp</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void dragRamp() {
  angle=Math.atan2(yHot,L-xHot);
  angle=Math.min(angle,Math.PI/2);
  angle=Math.max(angle,0);
  angleDegrees=Math.toDegrees(angle);
  xHot=L-L*Math.cos(angle);
  yHot=L*Math.sin(angle);
}
]]></Code>
</Content>
</Osejs.Model.Library.Page>
<Osejs.Model.Library.Page>
<Type>LIBRARY_EDITOR</Type>
<Name>Select Shape</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
public void selectShape () {
  if(shape.equals("Car")){
    maxX=-carSize/2;
    x=initX=(-L+carSize/2)/Math.cos(angle);
    y=0;
  }else if(shape.equals("Box")){
    maxX=-boxSize/2;
    x=initX=(-L+boxSize-0.15)/Math.cos(angle);
    y=0;
  }else if(shape.equals("Ball")){
    maxX=-boxSize/2;
    x=initX=(-L+boxSize-0.17)/Math.cos(angle);
    y=0;
  }else{
    shape="Box"; 
    x=initX=(-L+boxSize-0.15)/Math.cos(angle);
    maxX=-boxSize/2;
  }
  t=0;
  _initialize();
}
]]></Code>
</Content>
