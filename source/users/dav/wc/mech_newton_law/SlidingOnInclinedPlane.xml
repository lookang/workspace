<Name>Evolution</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<PreliminaryCode>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
double ax=g*Math.sin(angle);
if(motionDone){
  ax=0;
}else if(shape.equals("Car")){
  ax=TM*g/MI * Math.sin(angle);
}else if(shape.equals("Ball")){
  ax=(1.0/1.4)*g*Math.sin(angle);
}
]]></Code>
</PreliminaryCode>
