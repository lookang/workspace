public void experiment2() {
  g=0;
  double pos = xmin;
  for (int i = 0; i < n; i++) {
    x[i] = pos+0.5*diameter[i]; 
    pos += diameter[i];
    vx[i] = 1.0;
    y[i] = (ymin + ymax) * 0.5;
    vy[i] = 0.0;
    }
}
