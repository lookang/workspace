public void reverse () {
  // reverse the velocities
  for (int i=0; i<n; i++) {
    vx[i] *= -1;
    vy[i] *= -1;
  }
  blackTrail=!blackTrail;
  if(blackTrail) {
    _view.blackTrailSet.clear();
  } else {
    _view.redTrailSet.clear();
  }
}
