public void experiment1() {
  g=0;
  double posL = (xmax+xmin)/2, posR = posL;
  for (int i = 0; i < n; i++) {
    if (i%2==0) { x[i] = posL-diameter[i]/2; posL -= diameter[i]; }
    else { x[i] = posR+diameter[i]/2; posR += diameter[i]; }
    vx[i] = 0.0;
    y[i] = (ymin + ymax) * 0.5;
    vy[i] = 0.0;
    }
  x[n-2] = xmin+diameter[n-2];
  vx[n-2] = 1.0;
  x[n-1] = xmax-diameter[n-1];
  vx[n-1] = -1.0;
}
]]></Code>
