<Code><![CDATA[
public void drag () {
y2=y1+d0;
y=y2-h*d2/d1+d*d3/d1;
if(y2+d>y+h){
 y=y2+(1-d2/d1)*h/(1-d3/d1)-h;
}
if(y>y2) y=(y2+d-d2/d3*h);

if(y<y1)y=y1;

 c1=new java.awt.Color(c=200-(int)(d1/dmax*200),c,c,128);
 c2=new java.awt.Color(c=200-(int)(d2/dmax*200),c,c,128);
 c3=new java.awt.Color(c=200-(int)(d3/dmax*200),c,c,128);
 pressure=(d3*d+d1*(y2-y))*g;
 if(y2+d>y+h)pressure2=d3*(y2+d-y-h)*g;
 else pressure2=0.;
}
]]></Code>
