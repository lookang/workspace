  vsign=-1;
}else if(x<xmin-w/2 && vx<0){
  vx=-vx;
  w=-w;
  vsign=1.;
}
if(Math.abs(f2)<f2min){
  vhline=false;
  xi=-x;
  yi=y;
  wi=-w;
  hi=h;
  fmsg=l_msg;
  f=2*range;
}else{
  vhline=true;
  q1=1./(f2-1/(x+w2));
  q2=1./(f2-1./(x-w2));
  q=1./(f2-1/x);
  wi=q1-q2;
  xi=-q1+wi/2;
  if(y>0)ys=1;
  else ys=-1;  
  xi=-xi;
  if(f2<0){
    yi=-y;
    hi=-xi/x*h;
    yi=yi+ys*(h+hi)/2;
    wi=wi*vsign;
    if(xi<f)wi=-wi;
  }else{
//    xi=-xi;
    yi=y;
    hi=-xi/x*h;
    yi=yi-ys*(h-hi)/2;
    wi=wi*vsign;
  }
  fmsg="f="+dvalue(-f)+", p="+dvalue(-x)+", q="+dvalue(-q);
}
