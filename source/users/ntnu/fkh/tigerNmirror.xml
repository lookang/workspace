<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
double d,c;
public void dragf2(){
 f2=2/twof; setF();
}
public void dragf(){
 twof=2*f; dragf2();
// f2=1/f; setF();
}
public void setF () {
 if(Math.abs(f2)<f2min){
   d=(ymax-ymin)/(n-1);
   for(int i=0;i<n;i++){
      XP[i]=size/2; XP[n2-i-1]=-XP[i];
      YP[n2-i-1]=YP[i]=ymin+i*d;
   }
 }else{
   f=1/f2;
   c=Math.atan2(ymax/8.,-Math.abs(f));
   d=c/(n-1)/2;
   c=c/4;
   for(int i=0;i<n;i++){
      XP[i]=2*f*(1-Math.cos(c-i*d)); XP[n2-i-1]=XP[i]+size;
      YP[n2-i-1]=YP[i]=2*f*Math.sin(c-i*d);
   }
 }
twof=2*f;
}

public void drago(){
 for(int i=0;i<np;i++){
   if(Y[i]>0.1)Y[i]=0.1;
   else if(Y[i]<-0.1)Y[i]=-0.1;
   if(X[i]>0){
     X[i]=-size;
   }else{
    twof=f*2;
    if(f>0){
      if((X[i]-twof)*(X[i]-twof)+Y[i]*Y[i]<twof*twof){
       X[i]=X[i]-size;
      }
    }else if(f<0){
      if((X[i]-twof)*(X[i]-twof)+Y[i]*Y[i]>twof*twof){
       X[i]=X[i]-size;
      }
    }
   }
 }
}
]]></Code>
</Content>
