<Name>newrun</Name>
<Active>true</Active>
<Internal>false</Internal>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
int cnt=0,cnt2=0;
public void newrun () {
 testcount++;
 cnt2=0;
 for(int i=0;i<n;i++){
   if(i%n1==0)cnt=0;
   if(Math.random()>0.5){
    color[i]=red;
    cnt++;
    cnt2++;
   }else color[i]=blue;
   if(i%n1==(n1-1)){
     if(!hundred)count[cnt]+=1;
     txt[i/n1]=""+cnt;
   }
 }
 cmax=0;
 for(int i=0;i<n2+1;i++){
  if(count[i]>cmax)cmax=count[i];
 }
 if(hundred)count[cnt2]+=1;
 cmax=cmax+2;
 total=l_msg+cnt2;
 title=l_title+testcount;
}
]]></Code>
</Content>
