<Code><![CDATA[
int j;
testcount=0;
double d=range/(n1+1);
if(hundred){
  n2=n;
  n21=n2-1;
}else{
  n2=10;
  n21=n2-1;
}

for(int i=0;i<n+1;i++){
 xp[i]=i;
 count[i]=0;
}

for(int i=0;i<n1;i++) yp[i]=ymin+(i+1)*d;
cst=1./Math.sqrt(2*Math.PI);
for(int i=0;i<n;i++){
   j=i%n1;
   x[i]=xmin+d*(j+1);
   y[i]=ymin+(i/n1+1)*d;
}
red=new java.awt.Color(255,0,0);
blue=new java.awt.Color(0,0,255);
black=new java.awt.Color(0,0,0);
if(hundred){
 v=50.;
 clr=blue;
 sigma2=n;
}else{
  v=5;
  clr=black;
 cst=cst*Math.sqrt(1000);
 sigma2=10;
}
]]></Code>
