<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Orient Box</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
// renormalize to eliminate drift
double norm = 1/Math.sqrt(q[0]*q[0]+q[1]*q[1]+q[2]*q[2]+q[3]*q[3]); 
q[0] *= norm; q[1] *= norm;
q[2] *= norm; q[3] *= norm;
// update quaternion transformation for toSpaceFrame method
_view.bodyQuaternion.setCoordinates(q[0],q[1],q[2],q[3]);
//compute angular momentum in body frame for drawing
LBody= _view.bodyGroup.toBodyFrame(LSpace.clone());
omega[0]=LBody[0]/I1;  // compute angular velocity in body frame
omega[1]=LBody[1]/I2;
omega[2]=LBody[2]/I3;
// compute body vectors in space frame to draw trails
tipX= _view.bodyGroup.toSpaceFrame(new double[]{6,0,0});
tipY= _view.bodyGroup.toSpaceFrame(new double[]{0,6,0});
tipZ= _view.bodyGroup.toSpaceFrame(new double[]{0,0,6});

]]></Code>
</Content>
