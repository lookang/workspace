<Osejs.Model.Constraints>
<Osejs.Model.Constraints.Page>
<Type>CODE_EDITOR</Type>
<Name>Compute space and body frame vectors</Name>
<Active>true</Active>
<Content>
<Comment><![CDATA[]]></Comment>
<Code><![CDATA[
// renormalize to eliminate drift
double norm = 1/Math.sqrt(q[0]*q[0]+q[1]*q[1]+q[2]*q[2]+q[3]*q[3]); 
q[0] *= norm; q[1] *= norm;
q[2] *= norm; q[3] *= norm;
// compute current omega in body frame using the gyroscope quaternion
omega[0] = 2*(-q[1]*qDot[0]+q[0]*qDot[1]+q[3]*qDot[2]-q[2]*qDot[3]);
omega[1] = 2*(-q[2]*qDot[0]-q[3]*qDot[1]+q[0]*qDot[2]+q[1]*qDot[3]);
omega[2] = 2*(-q[3]*qDot[0]+q[2]*qDot[1]-q[1]*qDot[2]+q[0]*qDot[3]);
double[] L=new double[3];
L[0]=omega[0]*I1;                               // angular momentum body frame
L[1]=omega[1]*I2;
L[2]=omega[2]*I3;
angularMomentum[0]=L[0]*arrowScale;             // scaled vector for drawing
angularMomentum[1]=L[1]*arrowScale;
angularMomentum[2]=L[2]*arrowScale;
// orient view using current quaternion values so that transformations can be used immediately
_view.quaternion3D.setCoordinates(q[0],q[1],q[2],q[3]);  
// comptue shaft direction in space frame
double[] shaftDirection = _view.rotorGroup.toSpaceFrame(new double[]{0,0,1}); // transform 3-direction to space frame
shaftTip[0]=shaftDirection[0]*size;
shaftTip[1]=shaftDirection[1]*size;
shaftTip[2]=shaftDirection[2]*size;
// comptue torque in body frame
double[] zDirection= _view.rotorGroup.toBodyFrame(new double[]{0, 0, 1});  // transform z-direction to body frame
torque[0]=m*g*zDirection[1]*z0;                                            // compute cross product
torque[1]=-m*g*zDirection[0]*z0;
torque[2]=0;
// compute elevation angles
axisElevation=Math.acos(shaftDirection[2]);
double zDotL=L[0]*zDirection[0]+L[1]*zDirection[1]+L[2]*zDirection[2];
momentumElevation=Math.acos(zDotL/Math.sqrt(L[0]*L[0]+L[1]*L[1]+L[2]*L[2]));

]]></Code>
</Content>
